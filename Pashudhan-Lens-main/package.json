{
  "name": "cattle-buffalo-breed-recognition",
  "private": true,
  "version": "1.0.0",
  "description": "AI-powered breed identification system for Indian cattle and buffalo breeds supporting Bharat Pashudhan App",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "build:dev": "vite build --mode development",
    "build:analyze": "vite build && npx vite-bundle-analyzer dist",
    "preview": "vite preview --host 0.0.0.0",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "clean": "rimraf dist node_modules/.vite",
    "optimize": "npm run clean && npm run build",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "deps:update": "npx npm-check-updates -u",
    "deps:check": "npx npm-check-updates",
    "prepare": "husky install || exit 0",
    "pre-commit": "lint-staged",
    "validate": "npm run type-check && npm run lint:check && npm run test",
    "ci": "npm ci && npm run validate && npm run build"
  },
